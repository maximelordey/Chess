package piece;

import java.util.Vector;
import chess.Echiquier;
import mouve.Move;

public abstract class Piece {
	
	private Vector<Move> m_moves;
	private Echiquier.Couleur m_couleur;
	public Boolean first_move;
	
	public Piece(Vector<Move> moves) {
		m_moves=moves;
		first_move=false;
	}
	
	
	
}
